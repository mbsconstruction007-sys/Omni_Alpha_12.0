{
  "dashboard": {
    "id": null,
    "title": "Omni Alpha Enhanced Trading Dashboard",
    "tags": ["trading", "omni-alpha"],
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "Portfolio Value",
        "type": "stat",
        "targets": [
          {
            "expr": "omni_alpha_portfolio_value",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "custom": {
              "displayMode": "list",
              "placement": "bottom"
            },
            "mappings": [],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "color": "green",
                  "value": null
                },
                {
                  "color": "red",
                  "value": 80
                }
              ]
            },
            "unit": "currencyUSD"
          }
        },
        "gridPos": {
          "h": 8,
          "w": 6,
          "x": 0,
          "y": 0
        }
      },
      {
        "id": 2,
        "title": "Daily P&L",
        "type": "stat",
        "targets": [
          {
            "expr": "omni_alpha_daily_pnl",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "mappings": [],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "color": "red",
                  "value": null
                },
                {
                  "color": "yellow",
                  "value": 0
                },
                {
                  "color": "green",
                  "value": 100
                }
              ]
            },
            "unit": "currencyUSD"
          }
        },
        "gridPos": {
          "h": 8,
          "w": 6,
          "x": 6,
          "y": 0
        }
      },
      {
        "id": 3,
        "title": "Active Positions",
        "type": "stat",
        "targets": [
          {
            "expr": "omni_alpha_active_positions",
            "refId": "A"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 6,
          "x": 12,
          "y": 0
        }
      },
      {
        "id": 4,
        "title": "Win Rate",
        "type": "stat",
        "targets": [
          {
            "expr": "omni_alpha_win_rate",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent"
          }
        },
        "gridPos": {
          "h": 8,
          "w": 6,
          "x": 18,
          "y": 0
        }
      },
      {
        "id": 5,
        "title": "Portfolio Performance",
        "type": "timeseries",
        "targets": [
          {
            "expr": "omni_alpha_portfolio_value",
            "refId": "A",
            "legendFormat": "Portfolio Value"
          },
          {
            "expr": "omni_alpha_benchmark",
            "refId": "B",
            "legendFormat": "S&P 500 Benchmark"
          }
        ],
        "gridPos": {
          "h": 12,
          "w": 24,
          "x": 0,
          "y": 8
        }
      },
      {
        "id": 6,
        "title": "Trading Volume",
        "type": "timeseries",
        "targets": [
          {
            "expr": "omni_alpha_trade_count",
            "refId": "A"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 20
        }
      },
      {
        "id": 7,
        "title": "System Health",
        "type": "stat",
        "targets": [
          {
            "expr": "omni_alpha_system_health",
            "refId": "A"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 20
        }
      }
    ],
    "time": {
      "from": "now-24h",
      "to": "now"
    },
    "timepicker": {},
    "refresh": "30s",
    "version": 1
  }
}
